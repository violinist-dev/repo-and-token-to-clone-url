{
    "name": "violinist-dev/repo-and-token-to-clone-url",
    "description": "Given a repo and a token, what URL can you use to clone via https",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Violinist\\RepoAndTokenToCloneUrl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Violinist\\Tests\\RepoAndTokenToCloneUrl\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Eirik S. Morland",
            "email": "eirik@morland.no"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9 || ^10.5",
        "php-coveralls/php-coveralls": "^2.7",
        "slevomat/coding-standard": "^8.15",
        "squizlabs/php_codesniffer": "^3.11",
        "phpstan/phpstan": "^1.0 || ^2.0"
    },
    "scripts": {
        "test": "phpunit",
        "lint": "phpcs -p",
        "phpstan": "phpstan"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require": {
        "peterpostmann/parse_uri": "^1.0"
    }
}
